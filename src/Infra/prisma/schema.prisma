generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Author {
  id             String   @id
  name           String
  nationality    String
  booksPublished Int
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt

  @@map("authors")
}
